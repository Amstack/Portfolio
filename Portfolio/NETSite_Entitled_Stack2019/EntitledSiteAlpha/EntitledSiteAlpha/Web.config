<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns="http://schemas.microsoft.com/.NetConfiguration/v2.0">
  <system.web>
    <authorization>
      <allow users="*" />
    </authorization>
  </system.web>
  <location path="~/images/BankResources">
    <system.web>
      <authorization>
        <allow user="*"/>
      </authorization>
    </system.web>
  </location>
  <appSettings>
    <add key="EntitledDB" value="Data Source=localhost;Initial Catalog=EntitledDB;Integrated Security=True" />
  </appSettings>
</configuration>